
extension Array is
  init(input) do
    if input is Array then
      return [].add(array: input)
    else
      return [].add(count: input, value: nil)
    end
  end
end

var a := Array([1,2,3,4,5])
print(a)

var b := Array(5)
print(b)

a[1] := 9
print(a)

a += [1,2,3]
print(a)

b[1]:=9
b+=[4,5,6]
print(b)

